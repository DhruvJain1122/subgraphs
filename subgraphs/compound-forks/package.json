{
  "name": "compound-forks",
  "license": "MIT",
  "version": "1.0.0",
  "repository": "https://github.com/messari/subgraphs/blob/master/subgraphs/compound-forks",
  "scripts": {
    "format": "npx prettier --write .",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy/ compound-forks",
    "create-local": "graph create --node http://localhost:8020/ compound-forks",
    "remove-local": "graph remove --node http://localhost:8020/ compound-forks",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 compound-forks",
    "thegraph:deploy:mainnet-cream": "graph deploy dhruvjain1122/cream --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "prepare:mainnet-cream": "mustache config/cream/ethereum.json subgraph.template.yaml > subgraph.yaml",
    "build:mainnet-cream": "npm run prepare:mainnet-cream && graph build",
    "deploy:mainnet-cream": "npm run prepare:mainnet-cream && npm run build:mainnet-cream && npm run thegraph:deploy:mainnet-cream"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.28.1",
    "@graphprotocol/graph-ts": "0.26.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "mustache": "^4.2.0",
    "prettier": "^2.6.0",
    "typescript": "^4.6.2"
  }
}
